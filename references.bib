	keywords = {Android,Artificial neural network,Clustering,GPS,GSM,Location Based Services},
	mendeley-groups = {Skripsi},
	title = {{GPS based bus tracking system}},
	year = {2016}
}
@article{Najmurrokhman2021,
	abstract = {The number of traffic accidents in Indonesia is still relatively high. According to the official data, there were 107,500 cases of accidents in 2019. The accident was caused by various factors, including human factors as much as 61%. This indicates the driver's lack of awareness to comply with traffic regulations such as the maximum speed limit for vehicles. Based on these conditions, it is necessary to make a device that can observe the speed of the vehicle, so that the driver does not exceed the maximum speed limit of the vehicle on the highway. This paper describes on a prototype of the vehicle speed recorder using GPS tracker and Internet-of-Things (IoT) platform. GPS tracker determines the location of a vehicle any time. The speed of a vehicle is computed by using Arduino microcontroller and SIM808 module. Furthermore, the location and speed of a vehicle is sent to a dedicated webserver such that the IoT platform so called Adafruit IO could proceed and display how much the speed of a vehicle being traveled and its location in the real time. Experimental results show that the prototype of vehicle speed recorder can display a location with high accuracy and can render an average speed discrepancy of 3.9 kph during travelling at a certain route. The discrepancy between the value of speed and the location of a vehicle occurs due to the delay in sending latitude and longitude coordinate data from the SIM808 module to the IoT platform.},
	author = {Najmurrokhman, Asep and Kusnandar and Daelami, Ahmad and Komarudin, Udin and Imanudin, Muhamad},
	doi = {10.1109/ICAICST53116.2021.9497797},
	file = {:Users/airlanggafidiyanto/Documents/Kuliah/Skripsi/Referensi/Journals/Design_and_Implementation_of_Vehicle_Speed_Recorder_using_GPS_Tracker_and_Internet-of-Things_Platform.pdf:pdf},
	isbn = {9781665424042},
	journal = {ICAICST 2021 - 2021 International Conference on Artificial Intelligence and Computer Science Technology},
	keywords = {Adafruit IO,GPS tracker,SIM808 module,internet-of-Things,vehicle speed recorder},
	mendeley-groups = {Skripsi},
	pages = {152--156},
	publisher = {IEEE},
	title = {{Design and Implementation of Vehicle Speed Recorder using GPS Tracker and Internet-of-Things Platform}},
	year = {2021}
}
@article{Mukhtar2015,
	abstract = {— Security systems and navigators have always been a necessity of human " s life. The developments of advanced electronics have brought revolutionary changes in these fields. In this paper, we will present a vehicle tracking system that employs a GPS module and a GSM modem to find the location of a vehicle and offers a range of control features. To complete the design successfully, a GPS unit, two relays, a GSM Modem and two MCU units are used. There are five features introduced in the project. The aim of this project is to remotely track a vehicle " s location, remotely switch ON and OFF the vehicle " s ignition system and remotely lock and unlock the doors of the vehicle. An SMS message is sent to the tracking system and the system responds to the users request by performing appropriate actions. Short text messages are assigned to each of these features. A webpage is specifically designed to view the vehicle " s location on Google maps. By using relay based control concept introduced in this paper, number of control features such as turning heater on/off, radio on/off etc. can be implemented in the same fashion.},
	author = {Mukhtar, Mashood},
	doi = {10.5815/ijisa.2015.03.01},
